<?xml version="1.0" encoding="utf-8"?>
<manifest package="com.fjby.travel.leyou"
          xmlns:android="http://schemas.android.com/apk/res/android">

    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE"/>
    <uses-permission android:name="android.permission.READ_PHONE_STATE"/>
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE"/>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>

    <application
        android:name=".application.LeYouMyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme">
        <activity
            android:name=".activity.MainActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity
            android:name=".activity.FlushActivity"
            android:screenOrientation="portrait"
            />
        <activity
            android:name=".activity.HomeProduceActivity"
            android:screenOrientation="portrait"
            />
        <activity
            android:name=".activity.HomeLocationActivity"
            android:screenOrientation="portrait"
            />
        <activity
            android:name=".activity.LoginActivity"
            android:screenOrientation="portrait">
        </activity>
        <activity
            android:name=".activity.HomeTourActivity"
            android:screenOrientation="portrait">
        </activity>
        <activity
            android:name=".activity.RegisteredActivity"
            android:screenOrientation="portrait"
            >
        </activity>
        <activity
            android:name=".activity.HomeTourGuideActivity"
            android:screenOrientation="portrait">
        </activity>
        <activity
            android:name=".activity.HomeTourLocationActivity"
            android:screenOrientation="portrait"
            >
        </activity>
        <activity
            android:name=".activity.InfoMessageActivity"
            android:screenOrientation="portrait"
            android:windowSoftInputMode="adjustResize|stateHidden">
        </activity>
        <activity
            android:name=".activity.NotesDetailReviewsActivity"
            android:screenOrientation="portrait"
            android:windowSoftInputMode="adjustResize|stateHidden">
        </activity>
        <activity
            android:name=".activity.NotesDetailActivity"
            android:screenOrientation="portrait"
         >
        </activity>
        <activity
            android:name=".activity.InfoPushActivity"
            android:screenOrientation="portrait"
            >
        </activity>
        <activity
            android:name=".activity.NotesLocationActivity"
            android:screenOrientation="portrait"
            >
        </activity>
        <activity
            android:name=".activity.SettingActivity"
            android:screenOrientation="portrait"
            >
        </activity>
    </application>

</manifest>
