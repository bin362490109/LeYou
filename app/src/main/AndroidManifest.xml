<?xml version="1.0" encoding="utf-8"?>
<manifest package="com.fjby.travel.leyou"
          xmlns:android="http://schemas.android.com/apk/res/android">

    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE"/>
    <uses-permission android:name="android.permission.READ_PHONE_STATE"/>
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE"/>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>

    <application
        android:name="com.fjby.travel.leyou.application.LeYouMyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme">
        <activity
            android:name="com.fjby.travel.leyou.activity.FlushActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity
            android:name="com.fjby.travel.leyou.activity.MainActivity"
            android:screenOrientation="portrait"
            />
        <activity
            android:name="com.fjby.travel.leyou.activity.PassWordActivity"
            android:screenOrientation="portrait"
            />
        <activity
            android:name="com.fjby.travel.leyou.activity.HomeProduceActivity"
            android:screenOrientation="portrait"
            />
        <activity
            android:name="com.fjby.travel.leyou.activity.HomeLocationActivity"
            android:screenOrientation="portrait"
            />
        <activity
            android:name="com.fjby.travel.leyou.activity.LoginActivity"
            android:screenOrientation="portrait">
        </activity>
        <activity
            android:name="com.fjby.travel.leyou.activity.HomeTourActivity"
            android:screenOrientation="portrait">
        </activity>
        <activity
            android:name="com.fjby.travel.leyou.activity.HomeTourGuideActivity"
            android:screenOrientation="portrait">
        </activity>
        <activity
            android:name="com.fjby.travel.leyou.activity.HomeTourLocationActivity"
            android:screenOrientation="portrait"
            >
        </activity>
        <activity
            android:name="com.fjby.travel.leyou.activity.InfoMessageActivity"
            android:screenOrientation="portrait"
            android:windowSoftInputMode="adjustResize|stateHidden">
        </activity>
        <activity
            android:name="com.fjby.travel.leyou.activity.NotesDetailReviewsActivity"
            android:screenOrientation="portrait"
            android:windowSoftInputMode="adjustResize|stateHidden">
        </activity>
        <activity
            android:name="com.fjby.travel.leyou.activity.NotesDetailActivity"
            android:screenOrientation="portrait"
         >
        </activity>
        <activity
            android:name="com.fjby.travel.leyou.activity.InfoPushActivity"
            android:screenOrientation="portrait"
            >
        </activity>
        <activity
            android:name="com.fjby.travel.leyou.activity.NotesLocationActivity"
            android:screenOrientation="portrait"
            >
        </activity>
        <activity
            android:name="com.fjby.travel.leyou.activity.MySettingActivity"
            android:screenOrientation="portrait"
            >
        </activity>
        <activity
            android:name="com.fjby.travel.leyou.activity.MyTravelActivity"
            android:screenOrientation="portrait"
            >
        </activity>
        <activity
            android:name="com.fjby.travel.leyou.activity.MyCollectActivity"
            android:screenOrientation="portrait"
            >
        </activity>
        <activity
            android:name="com.fjby.travel.leyou.activity.MyAppealActivity"
            android:screenOrientation="portrait"
            >
        </activity>
        <activity
            android:name="com.fjby.travel.leyou.activity.MyRecordActivity"
            android:screenOrientation="portrait"
            android:windowSoftInputMode="adjustResize|stateHidden"
            >
        </activity>

        <activity
            android:name=".activity.SelectPic2Activity"
            android:theme="@style/DialogStyleBottom" >
        </activity>
    </application>

</manifest>
